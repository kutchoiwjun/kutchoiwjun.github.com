	case "tys": var hostname = "speedzone.tongyang.co.kr"; break;
	case "tyco": var hostname = "tyzone.tongyang.co.kr"; break;
	case "tysec" : var hostname = "myassetdgw1.tongyang.co.kr"; break;
	}

function IsUserName(entryName, entryKorName){
	try{
		if (entryName.toLowerCase().indexOf("cn=") >= 0){
			return true;

		//í•œê¸€ëª…ê³¼ ë‹¤ë¥´ë©´(LoginIDë‹ˆê¹Œ) íšŒì›
		}else if (entryName.toLowerCase()!=entryKorName.toLowerCase()){
			return true;

		}else{
			return false;
		}
	}catch (e){
		return false;
	}
}


.dijitCalendarIncrease .dijitCalendarIncreaseInner, .dijitCalendarDecrease .dijitCalendarDecreaseInner{display:none;}
.dijitCalendarYearContainer h3.dijitCalendarYearLabel{margin-bottom:0;}
/*show +/- in high contrast mode, and try to align them better in browsers that cooperate*/
.dijit_a11y .dijitCalendarIncrease .dijitCalendarIncreaseInner, .dijit_a11y .dijitCalendarDecrease .dijitCalendarDecreaseInner{display:inline;display:inline-block;margin-top:-8px;}
.dijit_a11y .dijitCalendarDecrease .dijitCalendarDecreaseInner{margin-top:-10px;margin-left:-10px}

/*lotusButton is a set width version of the button that goes in the left column*/
/*our regular buttons*/

/*2010 09 15 Sue */
/*.lotusBtn, .lotusButton{border-color:#c0c7cd;}default button outside border*/
.lotusBtn, .lotusButton{border-color:#D2D2D4;} /*default button outside border*/

/*.lotusBtn a, .lotusBtn a:visited, .lotusButton a, .lotusButton a:visited{border-color:#dedede;}span buttons have an inside border*/
.lotusBtn a, .lotusBtn a:visited, .lotusButton a, .lotusButton a:visited{border-color:#D2D2D4;}/*span buttons have an inside border*/

/*2010 09 15 Sue 컨텐츠 추가 버튼*/
/*.lotusBtn a, .lotusBtn a:visited, .lotu튼Button a, .lotusButton a:visited, input.lotusBtn, button.lotusBtn{color:#222;background-color:#dedede;background-image:url(../images/btnBkgd.png);}*/
.lotusBtn a, .lotusBtn a:visited, .lotusButton a, .lotusButton a:visited, input.lotusBtn, button.lotusBtn{background-color:#FFFFFF;width:130px;height:22px !important;color:#222222;background-image:url('/wps/images/KOR/button/btn1.gif');}

/*form and other blue buttons*/
/*.lotusBtnSpecial, .lotusFormButton, .lotusBtnImg{border-color:#2f76b9}*/
.lotusBtnSpecial, .lotusFormButton, .lotusBtnImg{border-color:#FFFFFF}

/*포탈 설정후 저장 관련 버튼*/
/*.lotusBtnSpecial a, .lotusBtnSpecial a:visited, .lotusBtnImg img, .lotusBtnImg input{color:#fff;border-color:#6cacf1;}*/ /*span buttons have an inside border*/
.lotusBtnSpecial a, .lotusBtnSpecial a:visited, .lotusBtnImg img, .lotusBtnImg input{color:#fff;border-color:#858585;} 


/*2010 09 15 설정변경시 저장버튼*/
/*.lotusBtnSpecial a, .lotusBtnSpecial a:visited, input.lotusBtnSpecial, button.lotusBtnSpecial, .lotusFormButton, .lotusBtnImg{color:#ffffff;background-color:#2c86eb;background-image:url(themeImages/btnSpecialBkgd.png);}*/
.lotusBtnSpecial a, .lotusBtnSpecial a:visited, input.lotusBtnSpecial, button.lotusBtnSpecial, .lotusFormButton{width:60px;height:22px;color:#7E7E7E;border-color:#D2D2D4;background-image:url('/wps/images/KOR/button/btn1.gif');}

/*hover states*/
/*default buttons*/
